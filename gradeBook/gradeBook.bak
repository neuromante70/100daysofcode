/*
Grade book
Complete the function so that it finds the average of the three scores passed to it
and returns the letter value associated with that grade.
Numerical Score	Letter Grade
90 <= score <= 100	'A'
80 <= score < 90	'B'
70 <= score < 80	'C'
60 <= score < 70	'D'
0 <= score < 60	'F'
Tested values are all between 0 and 100. Theres is no need to check for negative values
or values greater than 100.
*/

package main

import "fmt"

func GetGrade(a, b, c int) rune {
	// var grade rune

	average := (a + b + c) / 3
	// 90 <= score <= 100 'A'
	if average >= 90 {
		return 'A'
		// 80 <= score < 90	'B'
	} else if average >= 80 && average <= 90 {
		return 'B'
		// 70 <= score < 80	'C'
	} else if average >= 70 && average <= 80 {
		return 'C'
		// 60 <= score < 70	'D'
	} else if average >= 60 && average <= 70 {
		return 'D'
		// 0 <= score < 60	'F'
	} else if average >= 0 && average < 60 {
		return 'F'
	}
	return -1
}

func main() {
	// average := (a + b + c) / 3
	result := GetGrade(50, 50, 50)
	fmt.Println("")
	fmt.Println("Your grade is ", string(result))
}
